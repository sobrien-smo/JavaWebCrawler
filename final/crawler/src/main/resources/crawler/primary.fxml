<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="crawler.PrimaryController">
   <children>
      <Label text="Web Crawler" />
      
      <TextField fx:id="url1Input" text="URL 1" />
      <TextField fx:id="url2Input" text="URL 2" />
      <TextField fx:id="url3Input" text="URL 3" />

      <Button fx:id="addButton" text="add" onAction="#createURL"/>
      <TextField fx:id="searchTerm" />
      <Button fx:id="searchButton" text="search" onAction="#search"/>
      <Label text="response goes here" fx:id="responseText"/>
      <Label text="response goes here" fx:id="responseText2"/>

   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
